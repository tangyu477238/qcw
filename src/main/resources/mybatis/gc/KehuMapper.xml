<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bootdo.gc.dao.KehuDao">

	<select id="get" resultType="com.bootdo.gc.domain.KehuDO">
		select `id`,`bizdate`,`forwunit`,`carnum`,`station`,`tonnage`,`settletonnage`,
		       `adjusttonnage`,`transcost`,`inforfee`,`colmethod`,`transfee`,`riskcues`,
		       `fulltrans`,`fulpaydate`,`bankcard`,`bankpaydate`,`bankcardtwo`,`bankpaytwodate`,
		       `fultranstwo`,`fulpaytwodate`,`acbalance`,`receipt`,`receiptdate`,`carrier`
		       ,`remark` ,order_no

		from gc_kehu where id = #{value}
	</select>

	<select id="list" resultType="com.bootdo.gc.domain.KehuDO">
		select `id`,`bizdate`,`forwunit`,`carnum`,`station`,`tonnage`,`settletonnage`,
		       `adjusttonnage`,`transcost`,`inforfee`,`colmethod`,`transfee`,`riskcues`,
		       `fulltrans`,`fulpaydate`,`bankcard`,`bankpaydate`,`bankcardtwo`,`bankpaytwodate`,
		       `fultranstwo`,`fulpaytwodate`,`acbalance`,`receipt`,`receiptdate`,`carrier`,`remark`
		,order_no
		from gc_kehu
        <where>  
		  		  <if test="id != null and id != ''"> and id = #{id} </if>

		  		  <if test="startDate != null and startDate != ''"> and bizdate &gt;= #{startDate} </if>
			      <if test="endDate != null and endDate != ''"> and bizdate &lt;= #{endDate} </if>


		  		  <if test="forwunit != null and forwunit != ''"> and forwunit = #{forwunit} </if>
		  		  <if test="carnum != null and carnum != ''">
					  <![CDATA[ and carnum like CONCAT('%',#{carnum},'%') ]]>
				  </if>
		  		  <if test="station != null and station != ''"> and station = #{station} </if>
		  		  <if test="tonnage != null and tonnage != ''"> and tonnage = #{tonnage} </if>
		  		  <if test="settletonnage != null and settletonnage != ''"> and settletonnage = #{settletonnage} </if>
		  		  <if test="adjusttonnage != null and adjusttonnage != ''"> and adjusttonnage = #{adjusttonnage} </if>
		  		  <if test="transcost != null and transcost != ''"> and transcost = #{transcost} </if>
		  		  <if test="inforfee != null and inforfee != ''"> and inforfee = #{inforfee} </if>
		  		  <if test="colmethod != null and colmethod != ''"> and colmethod = #{colmethod} </if>
		  		  <if test="transfee != null and transfee != ''"> and transfee = #{transfee} </if>
		  		  <if test="riskcues != null and riskcues != ''"> and riskcues = #{riskcues} </if>
		  		  <if test="fulltrans != null and fulltrans != ''"> and fulltrans = #{fulltrans} </if>
		  		  <if test="fulpaydate != null and fulpaydate != ''"> and fulpaydate = #{fulpaydate} </if>
		  		  <if test="bankcard != null and bankcard != ''"> and bankcard = #{bankcard} </if>
		  		  <if test="bankpaydate != null and bankpaydate != ''"> and bankpaydate = #{bankpaydate} </if>
		  		  <if test="bankcardtwo != null and bankcardtwo != ''"> and bankcardtwo = #{bankcardtwo} </if>
		  		  <if test="bankpaytwodate != null and bankpaytwodate != ''"> and bankpaytwodate = #{bankpaytwodate} </if>
		  		  <if test="fultranstwo != null and fultranstwo != ''"> and fultranstwo = #{fultranstwo} </if>
		  		  <if test="fulpaytwodate != null and fulpaytwodate != ''"> and fulpaytwodate = #{fulpaytwodate} </if>
		  		  <if test="acbalance != null and acbalance != ''"> and acbalance = #{acbalance} </if>
		  		  <if test="receipt != null and receipt != ''"> and receipt = #{receipt} </if>
		  		  <if test="receiptdate != null and receiptdate != ''"> and receiptdate = #{receiptdate} </if>
		  		  <if test="carrier != null and carrier != ''">

					  <![CDATA[ and carrier like CONCAT('%',#{carrier},'%') ]]>
				  </if>
		  		  <if test="remark != null and remark != ''"> and remark = #{remark} </if>
		  		</where>
        <choose>
            <when test="sort != null and sort.trim() != ''">
                order by ${sort} ${order}
            </when>
			<otherwise>
                order by id desc
			</otherwise>
        </choose>
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>
	
 	<select id="count" resultType="int">
		select count(*) from gc_kehu
		 <where>  
		  		  <if test="id != null and id != ''"> and id = #{id} </if>
		  		  <if test="bizdate != null and bizdate != ''"> and bizdate = #{bizdate} </if>
		  		  <if test="forwunit != null and forwunit != ''"> and forwunit = #{forwunit} </if>
		  		  <if test="carnum != null and carnum != ''"> and carnum = #{carnum} </if>
		  		  <if test="station != null and station != ''"> and station = #{station} </if>
		  		  <if test="tonnage != null and tonnage != ''"> and tonnage = #{tonnage} </if>
		  		  <if test="settletonnage != null and settletonnage != ''"> and settletonnage = #{settletonnage} </if>
		  		  <if test="adjusttonnage != null and adjusttonnage != ''"> and adjusttonnage = #{adjusttonnage} </if>
		  		  <if test="transcost != null and transcost != ''"> and transcost = #{transcost} </if>
		  		  <if test="inforfee != null and inforfee != ''"> and inforfee = #{inforfee} </if>
		  		  <if test="colmethod != null and colmethod != ''"> and colmethod = #{colmethod} </if>
		  		  <if test="transfee != null and transfee != ''"> and transfee = #{transfee} </if>
		  		  <if test="riskcues != null and riskcues != ''"> and riskcues = #{riskcues} </if>
		  		  <if test="fulltrans != null and fulltrans != ''"> and fulltrans = #{fulltrans} </if>
		  		  <if test="fulpaydate != null and fulpaydate != ''"> and fulpaydate = #{fulpaydate} </if>
		  		  <if test="bankcard != null and bankcard != ''"> and bankcard = #{bankcard} </if>
		  		  <if test="bankpaydate != null and bankpaydate != ''"> and bankpaydate = #{bankpaydate} </if>
		  		  <if test="bankcardtwo != null and bankcardtwo != ''"> and bankcardtwo = #{bankcardtwo} </if>
		  		  <if test="bankpaytwodate != null and bankpaytwodate != ''"> and bankpaytwodate = #{bankpaytwodate} </if>
		  		  <if test="fultranstwo != null and fultranstwo != ''"> and fultranstwo = #{fultranstwo} </if>
		  		  <if test="fulpaytwodate != null and fulpaytwodate != ''"> and fulpaytwodate = #{fulpaytwodate} </if>
		  		  <if test="acbalance != null and acbalance != ''"> and acbalance = #{acbalance} </if>
		  		  <if test="receipt != null and receipt != ''"> and receipt = #{receipt} </if>
		  		  <if test="receiptdate != null and receiptdate != ''"> and receiptdate = #{receiptdate} </if>
		  		  <if test="carrier != null and carrier != ''"> and carrier = #{carrier} </if>
		  		  <if test="remark != null and remark != ''"> and remark = #{remark} </if>
		  		</where>
	</select>
	 
	<insert id="save" parameterType="com.bootdo.gc.domain.KehuDO" useGeneratedKeys="true" keyProperty="id">
		insert into gc_kehu
		(
			`bizdate`, 
			`forwunit`, 
			`carnum`, 
			`station`, 
			`tonnage`, 
			`settletonnage`, 
			`adjusttonnage`, 
			`transcost`, 
			`inforfee`, 
			`colmethod`, 
			`transfee`, 
			`riskcues`, 
			`fulltrans`, 
			`fulpaydate`, 
			`bankcard`, 
			`bankpaydate`, 
			`bankcardtwo`, 
			`bankpaytwodate`, 
			`fultranstwo`, 
			`fulpaytwodate`, 
			`acbalance`, 
			`receipt`, 
			`receiptdate`, 
			`carrier`, 
			`remark`,
		 	 order_no
		)

	<delete id="remove">
		delete from gc_kehu where id = #{value}
	</delete>
	
	<delete id="batchRemove">
		delete from gc_kehu where id in 
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</delete>



